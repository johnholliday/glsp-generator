// Validation Examples
// This file demonstrates both valid and invalid model configurations

// ============================================
// VALID EXAMPLES
// ============================================

// Valid: All required properties present
Workflow validComplete {
    name: "value1"
    version: "value1"
    description: "value1"
    tasks: [@task1]
    flows: [@flow1]
    startTask: @task1
    endTasks: [@task1]
}

// Valid: With optional properties
Workflow validWithOptional {
    name: "value1"
    version: "value1"
    description: "value1"
    tasks: [@task1]
    flows: [@flow1]
    startTask: @task1
    endTasks: [@task1]
}

